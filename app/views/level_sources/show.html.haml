- content_for :head do
  - if @game.app == "turtle"
    = tag('meta', :property => 'og:image', :content => show_image)
    = tag('meta', :name => 'twitter:card', :content => "photo")
    = tag('meta', :name => 'twitter:image', :content => show_image)
- @page_title = t('share_code.title')

%h1= @page_title
= render partial: "levels/blockly", locals: { start_blocks: @start_blocks, app: @game.app, hide_source: @hide_source, share: @share }

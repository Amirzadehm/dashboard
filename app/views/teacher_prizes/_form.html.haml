= form_for @teacher_prize do |f|
  - if @teacher_prize.errors.any?
    #error_explanation
      %h2= "#{pluralize(@teacher_prize.errors.count, "error")} prohibited this teacher_prize from being saved:"
      %ul
        - @teacher_prize.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :prize_provider
    = f.text_field :prize_provider
  .field
    = f.label :code
    = f.text_field :code
  .field
    = f.label :user
    = f.text_field :user
  .actions
    = f.submit 'Save'
